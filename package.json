{
  "name": "antaeus",
  "version": "0.7.2",
  "description": "IPFS gateway",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "precommit": [
    "npm run lint",
    "npm test"
  ],
  "bin": {
    "antaeus": "src/cli/bin.js",
    "antaeusctl": "src/cli/client.js"
  },
  "scripts": {
    "lint": "aegir-lint",
    "release": "aegir-release --env node",
    "post-release": "./script/publish_antaeus_docker",
    "build": "aegir-build",
    "test": "aegir-test node",
    "coverage": "aegir-coverage",
    "coverage-publish": "aegir-coverage publish",
    "start": "node src/index.js",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "github.com/kanej/antaeus"
  },
  "keywords": [
    "IPFS"
  ],
  "author": "John Kane <john@kanej.me>",
  "license": "MIT",
  "devDependencies": {
    "aegir": "^11.0.1",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "2.0.0",
    "mocha": "3.2.0",
    "mocha-junit-reporter": "^1.13.0",
    "nock": "9.0.11",
    "precommit": "^1.2.2",
    "sinon": "2.1.0"
  },
  "dependencies": {
    "bluebird": "3.5.0",
    "body-parser": "1.17.1",
    "express": "^4.15.2",
    "ipfs-api": "14.0.0",
    "is-ipfs": "0.3.0",
    "jsonapi-serializer": "3.4.2",
    "jsonwebtoken": "7.3.0",
    "lodash": "^4.17.4",
    "morgan": "1.8.1",
    "nconf": "0.8.4",
    "node-etcd": "5.0.3",
    "osenv": "0.1.4",
    "request": "2.81.0",
    "ronin": "^0.3.11",
    "winston": "2.3.1",
    "yargs": "5.0.0"
  },
  "contributors": [
    "John Kane <john@kanej.me>"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}