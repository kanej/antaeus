{
  "name": "antaeus",
  "version": "0.6.0",
  "description": "IPFS gateway",
  "main": "src/index.js",
  "jsnext:main": "src/index.js",
  "precommit": [
    "npm run lint",
    "npm test"
  ],
  "bin": {
    "antaeus": "src/cli/bin.js"
  },
  "scripts": {
    "lint": "aegir-lint",
    "release": "aegir-release --env node",
    "post-release": "./script/publish_antaeus_docker",
    "build": "aegir-build",
    "test": "aegir-test node",
    "coverage": "aegir-coverage",
    "coverage-publish": "aegir-coverage publish",
    "start": "node src/index.js",
    "commit": "git-cz"
  },
  "repository": {
    "type": "git",
    "url": "github.com/kanej/antaeus"
  },
  "keywords": [
    "IPFS"
  ],
  "author": "John Kane <john@kanej.me>",
  "license": "MIT",
  "devDependencies": {
    "aegir": "^4.0.0",
    "chai": "^3.5.0",
    "cz-conventional-changelog": "1.1.6",
    "mocha": "^2.5.3",
    "mocha-junit-reporter": "^1.11.1",
    "nock": "8.0.0",
    "precommit": "^1.2.0",
    "sinon": "^1.17.4"
  },
  "dependencies": {
    "bluebird": "3.4.1",
    "body-parser": "1.15.2",
    "express": "^4.13.4",
    "ipfs-api": "6.0.3",
    "jsonapi-serializer": "3.3.0",
    "jsonwebtoken": "7.1.6",
    "lodash": "^4.13.1",
    "morgan": "1.7.0",
    "node-etcd": "5.0.3",
    "request": "2.74.0",
    "ronin": "^0.3.11",
    "winston": "2.2.0"
  },
  "contributors": [
    "John Kane <john@kanej.me>"
  ],
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}